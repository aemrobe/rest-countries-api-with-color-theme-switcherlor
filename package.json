{
  "name": "rest-countries-api-with-color-theme-switcher-master",
  "version": "1.0.0",
  "description": "an application which displays an information about a countries",
  "default": "index.html",
  "scripts": {
    "start": "run-script-os",
    "start-parcel": "parcel index.html",
    "build": "parcel build index.html --dist-dir ./dist",
    "start:win32": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --directory --port 7777 --browser \"C:\\Program Files\\Google\\Chrome\\Application\\chrome.exe\"",
    "//": "Hello! If you are having trouble running this command. Try changing Firefox Developer Edition to FirefoxDeveloperEdition",
    "start:darwin:linux": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --directory --port 7777 --browser 'Chrome'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "browser-sync": "^3.0.2",
    "core-js": "^3.35.1",
    "parcel": "^2.11.0",
    "regenerator-runtime": "^0.14.1",
    "run-script-os": "^1.1.6"
  }
}
